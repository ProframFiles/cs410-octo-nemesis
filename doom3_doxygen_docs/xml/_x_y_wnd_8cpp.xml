/*
<sp/>=======================================================================================================================
<sp/>=======================================================================================================================
<sp/>*/
void<sp/>CXYWnd::SplitClip()<sp/>{
<sp/><sp/><sp/>ProduceSplitLists();
<sp/><sp/><sp/>if<sp/>((g_brFrontSplits.next<sp/>!=<sp/>&amp;g_brFrontSplits)<sp/>&amp;&amp;<sp/>(g_brBackSplits.next<sp/>!=<sp/>&amp;g_brBackSplits))<sp/>{
<sp/><sp/><sp/><sp/><sp/><sp/>Select_Delete();
<sp/><sp/><sp/><sp/><sp/><sp/><sp/>Brush_CopyList(&amp;g_brFrontSplits,<sp/>&amp;selected_brushes);
<sp/><sp/><sp/><sp/><sp/><sp/><sp/>Brush_CopyList(&amp;g_brBackSplits,<sp/>&amp;selected_brushes);
